options:
  parameters:
    author: Deepwave Digital, Inc
    catch_exceptions: 'True'
    category: Custom
    cmake_opt: ''
    comment: ''
    copyright: ''
    description: Example for Wavelearner Inference Block
    gen_cmake: 'On'
    gen_linking: dynamic
    generate_options: no_gui
    hier_block_src_path: '.:'
    id: classifier_test
    max_nouts: '0'
    output_language: python
    placement: (0,0)
    qt_qss_theme: ''
    realtime_scheduling: ''
    run: 'True'
    run_command: '{python} -u {filename}'
    run_options: run
    sizing_mode: fixed
    thread_safe_setters: ''
    title: Classifier Test
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [8, 22]
    rotation: 0
    state: enabled

blocks:
- name: batch_size
  id: variable
  parameters:
    comment: ''
    value: '256'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [512, 68]
    rotation: 0
    state: enabled
- name: frequency
  id: variable
  parameters:
    comment: ''
    value: 2.4e9
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [776, 68]
    rotation: 0
    state: enabled
- name: input_len
  id: variable
  parameters:
    comment: ''
    value: '2048'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [216, 68]
    rotation: 0
    state: enabled
- name: output_len
  id: variable
  parameters:
    comment: ''
    value: '12'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [360, 68]
    rotation: 0
    state: enabled
- name: samp_rate
  id: variable
  parameters:
    comment: ''
    value: 31.25e6
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [648, 68]
    rotation: 0
    state: enabled
- name: blocks_file_sink_0
  id: blocks_file_sink
  parameters:
    affinity: ''
    alias: ''
    append: 'False'
    comment: ''
    file: output.bin
    type: float
    unbuffered: 'False'
    vlen: output_len*batch_size
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [888, 172.0]
    rotation: 0
    state: enabled
- name: buffer0
  id: blocks_stream_to_vector
  parameters:
    affinity: ''
    alias: ''
    comment: ''
    maxoutbuf: '0'
    minoutbuf: '0'
    num_items: batch_size*input_len
    type: complex
    vlen: '1'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [448, 200.0]
    rotation: 0
    state: enabled
- name: inference
  id: wavelearner_inference
  parameters:
    affinity: ''
    alias: ''
    batch_size: batch_size
    comment: ''
    input_type: complex
    input_vlen: input_len*batch_size
    maxoutbuf: '0'
    minoutbuf: '0'
    output_vlen: output_len*batch_size
    plan_filepath: saved_model.plan
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [624, 188.0]
    rotation: 0
    state: enabled
- name: soapy_source_0
  id: soapy_source
  parameters:
    affinity: ''
    alias: ''
    ant0: ''
    ant1: ''
    balance0: '0'
    balance1: '0'
    bw0: '0'
    bw1: '0'
    center_freq0: frequency
    center_freq1: '0'
    clock_rate: '0'
    clock_source: ''
    comment: ''
    correction0: '0'
    correction1: '0'
    dc_offset0: '0'
    dc_offset1: '0'
    dc_offset_auto_mode0: 'True'
    dc_offset_auto_mode1: 'False'
    dev: '"device=SoapyAIRT"'
    fine_tune0: 'False'
    fine_tune1: 'False'
    gain_auto_mode0: 'True'
    gain_auto_mode1: 'False'
    lna_gain0: '0'
    lna_gain1: '0'
    maxoutbuf: '0'
    minoutbuf: '0'
    mix_gain0: '0'
    mix_gain1: '0'
    nchan: '1'
    overall_gain0: '0'
    overall_gain1: '0'
    pga_gain0: '0'
    pga_gain1: '0'
    samp_rate: samp_rate
    tia_gain0: '0'
    tia_gain1: '0'
    tuner_gain0: '0'
    tuner_gain1: '0'
    type: fc32
    vga_gain0: '0'
    vga_gain1: '0'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [16, 199]
    rotation: 0
    state: enabled

connections:
- [buffer0, '0', inference, '0']
- [inference, '0', blocks_file_sink_0, '0']
- [soapy_source_0, '0', buffer0, '0']

metadata:
  file_format: 1
